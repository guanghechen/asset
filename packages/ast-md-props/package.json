{
  "name": "@guanghechen/ast-md-props",
  "version": "0.0.0",
  "description": "parse markdown to component props ast",
  "homepage": "https://github.com/lemon-clown/static-site-api/tree/master/packages/ast-md-props",
  "private": true,
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/static-site-api.git"
  },
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/ && yarn test",
    "build": "rollup -c rollup.config.js",
    "test": "TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir .",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build"
  },
  "dependencies": {
    "@types/mdast": "^3.0.3",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.39",
    "@barusu/rollup-config": "^0.0.39",
    "cross-env": "~7.0.2",
    "rollup": "2.28.1"
  }
}
