{
  "name": "@guanghechen/asset-markdown-parser",
  "version": "0.0.0",
  "description": "parse markdown to component props ast",
  "homepage": "https://github.com/lemon-clown/static-site-api/tree/master/packages/asset-markdown-parser",
  "private": true,
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/static-site-api.git"
  },
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir .",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build"
  },
  "dependencies": {
    "@types/mdast": "^3.0.3",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "@barusu/rollup-config": "^0.0.46",
    "cross-env": "~7.0.2",
    "rollup": "2.28.1"
  }
}
