{
  "name": "@guanghechen/asset-file",
  "version": "0.0.0",
  "description": "file asset",
  "homepage": "https://github.com/lemon-clown/static-site-api/tree/master/packages/asset-file#readme",
  "private": true,
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/static-site-api.git"
  },
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -w -c ../../rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir .",
    "update:test": "node -r ts-node/register -r tsconfig-paths/register __test__/answer.ts"
  },
  "dependencies": {
    "@guanghechen/site-api": "^0.0.0",
    "@types/micromatch": "^4.0.1",
    "micromatch": "^4.0.2"
  }
