{
  "name": "@guanghechen/asset",
  "version": "0.0.0",
  "description": "api of static site",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/asset.git"
  },
  "homepage": "https://github.com/guanghechen/asset/#readme",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 16.15.1"
  },
  "scripts": {
    ":publish": "yarn test && lerna publish",
    ":upgrade": "yarn upgrade-interactive && run-s clean format build test",
    "build": "lerna run build --concurrency 1",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lintfix format:prettier",
    "format:prettier": "prettier . --write",
    "format:lintfix": "eslint . --fix",
    "prepublishOnly": "pinst --disable",
    "postinstall": "run-s postinstall:dev",
    "postinstall:dev": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "rebuild": "run-s clean build",
    "run:ts": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node --loader ./loader.js",
    "test": "lerna run test --concurrency 1",
    "test:coverage": "lerna run --concurrency 1 test -- --coverage",
    "test:update": "lerna run --concurrency 1 test -- -u",
    "update:changelog": "gitmoji-changelog update"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "7.19.1",
    "@guanghechen/eslint-config": "^4.6.2",
    "@guanghechen/eslint-config-ts": "^4.6.2",
    "@guanghechen/invariant": "^4.6.2",
    "@guanghechen/jest-config": "^4.6.2",
    "@guanghechen/rollup-config": "^4.6.2",
    "@guanghechen/rollup-config-cli": "^4.6.2",
    "@types/jest": "^29.4.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.15.0",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "gitmoji-changelog": "^2.3.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "29.5.0",
    "lerna": "^6.5.1",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "2.8.4",
    "rimraf": "~4.4.0",
    "rollup": "3.19.1",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.3.1"
}
